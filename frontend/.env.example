VITE_BACKEND_URL=http://localhost:5000

# Environment Configuration Example
# Copy this file to .env.local and configure your values

# Application Information
REACT_APP_NAME="AI Chatbot Socket"
REACT_APP_VERSION="1.0.0"

# Server URLs
REACT_APP_API_URL=http://localhost:5000
REACT_APP_SOCKET_URL=http://localhost:5000

# Logging Configuration
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_LOGGING=true
REACT_APP_ENABLE_CONSOLE_LOGS=true

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true
REACT_APP_ENABLE_DEV_TOOLS=true

# UI Settings
REACT_APP_DEFAULT_THEME=system
REACT_APP_ENABLE_ANIMATIONS=true
REACT_APP_ANIMATION_DURATION=300

# Chat Settings
REACT_APP_MAX_MESSAGE_LENGTH=4000
REACT_APP_TYPING_TIMEOUT=3000

# Performance
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
